{
  "name": "enterprise-admin-panel",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^1.3.0",
    "chart.js": "^4.2.0",
    "date-fns": "^2.29.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts build && npm run copy-sw",
    "build:prod": "NODE_ENV=production npm run build && npm run setup-prod",
    "copy-sw": "cp public/sw.js build/sw.js",
    "setup-prod": "echo 'Production build complete. Use npm run serve to test.'",
    "serve": "npx serve -s build -l 3000",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "ajv": "^8.17.1",
    "cross-env": "^10.0.0"
  }
}
